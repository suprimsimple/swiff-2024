{
  "name": "swiff-4",
  "version": "3.0.0",
  "main": "build/index.js",
  "type": "module",
  "description": "Swiff saves you time with common SSH tasks during the development of websites/apps",
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
